{
  "name": "SEU Login",
  "description": "SEU auto login",
  "version": "2.0",
  "icons":{"16":"icon_disable.png"},
  "content_scripts":[
	{
		"matches":["https://w.seu.edu.cn/*"],
		"js":["jquery-1.7.1.min.js", "login.js"],
		"all_frames":true
	}
  ],
  "permissions": [
    "activeTab",
	"storage",
	"nativeMessaging"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon":"icon_disable.png",
    "default_title": "SEU Login"
  },
  "manifest_version": 2
}
